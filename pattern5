/* PRINT THIS PATTERN
****
***
**
*
*/
import java.util.Scanner;

class pattern11
{
    public static void main(String[] args) 
    {
        System.out.println("enter N");
        int n;
        Scanner sc = new Scanner(System.in);
        n= sc.nextInt();
        for(int i=1; i<=n; i++)
        {
            for(int j=n; j>=i; j--)
            {
                System.out.print("*");    
            }
            System.out.println("");
        }
        
        }
        
    }
